{
  "manifest_version": 2,

  "name": "S4L",
  "description": "This extension lets you to save your tabs for later checks.",
  "version": "1.0.2",
  "permissions": [
    "tabs",
	"storage",
	"unlimitedStorage"
  ],
  "background": {
    "persistent": false,
	"scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icons/store.png"
  }
}